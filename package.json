{
  "name": "tasks-with-friends",
  "version": "0.0.1",
  "description": "Blank web application built with Typescript and LESS",
  "main": "index.js",
  "scripts": {
    "data": "ts-node ./src/tools/convert.ts",
    "prebuild": "run-s lint clean build-info build:tailwind",
    "build": "run-s build:*",
    "build:types": "ility -l typescript -i domain/v1/schema.oas2.json -o src/domain",
    "postbuild:types": "rm src/domain/v1/express-routers.ts && rm src/domain/v1/validators.ts && mv src/domain/v1/types.ts src/domain/v1/api.g.ts",
    "build:graphql": "generate-server-types -i domain/v1/schema.graphql -o src/domain/v1/graph.g.ts",
    "prebuild:client-types": "run-s clean:client-types",
    "build:client-types": "npx apollo codegen:generate --localSchemaFile=domain/v1/schema.graphql --target=typescript --tagName=gql",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:functions": "webpack --config webpack.functions.js",
    "postbuild:functions": "ts-node tools/strip-require ./build/functions/auth.js && ts-node tools/strip-require ./build/functions/graphql.js",
    "//build:tsc": "tsc -p tsconfig.build.json",
    "build:tailwind": "tailwindcss -i src/tailwind.css -o build/dist/tailwind.css",
    "clean": "run-s clean:*",
    "clean:build": "rimraf build",
    "clean:client-types": "find . -name __generated__ | xargs rm -rf",
    "copyfiles": "run-s copyfiles:*",
    "copyfiles:static": "copyfiles -u 1 static/**.* static/* build/dist",
    "build-info": "ts-node tools/build-info.ts > src/build-info.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src/**/*.ts",
    "prenetlify": "run-s build",
    "netlify": "dotenv -e secrets.env netlify dev",
    "postbuild": "run-s copyfiles",
    "pretest": "rm -rf coverage/*",
    "test": "dotenv -e secrets.env jest",
    "prestart": "run-s build-info copyfiles",
    "start": "run-s watch",
    "watch": "dotenv -e secrets.env concurrently npm:watch:*",
    "watch:graphql": "run-s \"build:graphql -- --watch\"",
    "watch:tailwind": "run-s \"build:tailwind -- --watch\"",
    "watch:webpack": "webpack serve --config webpack.dev.js",
    "provision": "dotenv -e secrets.env ts-node tools/provision.ts"
  },
  "keywords": [],
  "author": "",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@netlify/functions": "^0.10.0",
    "@types/cookie-session": "^2.0.43",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^14.14.9",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-oauth2-refresh": "^1.1.1",
    "@types/pg": "^8.6.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "ility": "^0.0.8",
    "jest": "^26.6.3",
    "netlify-cli": "^8.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@analytics/google-analytics": "^0.5.3",
    "@apollo/client": "^3.5.6",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "analytics": "^0.7.19",
    "analytics-plugin-do-not-track": "^0.1.5",
    "cookie-session": "^2.0.0",
    "dataloader": "^2.0.0",
    "express": "^4.17.2",
    "faunadb": "^4.4.1",
    "graphql": "^16.2.0",
    "graphql-server-types": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-google-oauth": "^2.0.0",
    "passport-oauth2-refresh": "^2.1.0",
    "pg": "^8.7.1",
    "pusher": "^5.0.0",
    "pusher-js": "^7.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "serverless-http": "^2.7.0",
    "tailwindcss": "^3.0.0",
    "ts-registry": "^1.0.3",
    "ts-registry-express": "^1.0.2",
    "use-analytics": "^0.0.5",
    "use-http": "^1.0.26",
    "uuid": "^8.3.2"
  }
}
